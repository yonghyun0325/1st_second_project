<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  
  <!-- MyBatis에서 데이터 타입으로 사용할 객체들에 대한 별칭(alias) 정의 -->
  <typeAliases>
  	<typeAlias alias="calendarVO" type="com.human.web.vo.CalendarVO"/>
  	<typeAlias alias="employeesVO" type="com.human.web.vo.EmployeesVO"/>
  	<typeAlias alias="boardVO" type="com.human.web.vo.BoardVO"/>
  	<typeAlias alias="noticeVO" type="com.human.web.vo.NoticeVO"/>
  	<typeAlias alias="naVO" type="com.human.web.vo.NoticeAttachedVO"/>
  	
  </typeAliases>
  
</configuration>